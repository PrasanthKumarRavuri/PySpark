{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-staging-batch"
		},
		"ls_OnpremFile_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_OnpremFile'"
		},
		"ls_adlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsgen2'"
		},
		"ls_azureBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azureBlob'"
		},
		"ls_oracle_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_oracle'"
		},
		"ls_OnpremFile_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\ADE\\PysparkTelugu-master"
		},
		"ls_OnpremFile_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "HP"
		},
		"ls_adlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgen2storrage.dfs.core.windows.net/"
		},
		"ls_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://akvbatch27.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-staging-batch_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adlsgen2_accountKey": {
						"value": "[parameters('ls_adlsgen2_accountKey')]"
					},
					"ls_azureBlob_connectionString": {
						"value": "[parameters('ls_azureBlob_connectionString')]"
					},
					"ls_OnpremFile_password": {
						"value": "[parameters('ls_OnpremFile_password')]"
					},
					"ls_oracle_connectionString": {
						"value": "[parameters('ls_oracle_connectionString')]"
					},
					"ls_adlsgen2_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsgen2_properties_typeProperties_url')]"
					},
					"ls_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_OnpremFile_properties_typeProperties_host": {
						"value": "[parameters('ls_OnpremFile_properties_typeProperties_host')]"
					},
					"ls_OnpremFile_properties_typeProperties_userId": {
						"value": "[parameters('ls_OnpremFile_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}